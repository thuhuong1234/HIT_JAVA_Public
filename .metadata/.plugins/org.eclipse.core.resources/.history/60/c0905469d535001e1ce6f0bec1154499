package Btvn;

import java.util.Scanner;

public class Bai1 {
	public static void main(String[] args) {
		Scanner myObj = new Scanner(System.in);
		int n;
		do {
			System.out.print("n = ");
			n = myObj.nextInt();
		}while(n<=0);
		float S=0;
		for(int i =1; i<=n; i++) {
			S+=1/i;
		}
		System.out.printf("Tong S = %f",S);
	}
}
