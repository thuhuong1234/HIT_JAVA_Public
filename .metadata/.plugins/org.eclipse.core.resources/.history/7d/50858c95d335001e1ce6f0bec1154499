package Btvn;

import java.util.Scanner;

public class Bai1 {
	public static void main(String[] args) {
		Scanner myObj1 = new Scanner(System.in);
		System.out.print("n = ");
		int n = myObj1.nextInt();
		for (int j = 0; j < n; j++) {
			for (int i = 0; i < n; i++) {
				System.out.print("*");
			}
		}

	}
}
